{
  "name": "material-ui-panel-manager",
  "version": "0.0.6",
  "description": "<MuiPanel> component for material-ui",
  "main": "lib/index.js",
  "private": false,
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/types": "^7.14.8",
    "babel-eslint": "^10.0.3",
    "babel-loader": "8.2.2",
    "concurrently": "^6.2.0",
    "copy-and-watch": "^0.1.5",
    "npm-check-updates": "^11.8.3"
  },
  "dependencies": {
    "@material-ui/core": ">=4.12.3",
    "@material-ui/icons": ">=4.11.2",
    "lodash.get": ">=4.4.2",
    "prop-types": "^15.7.2",
    "react": ">=17.0.2"
  },
  "files": [
    "LICENSE",
    "*.md",
    "lib",
    "src"
  ],
  "keywords": [
    "reactjs",
    "react",
    "icon",
    "icons",
    "material-ui"
  ],
  "author": "rand0mC0d3r",
  "license": "MIT",
  "scripts": {
    "dev": "ncu -u && concurrently \"npm run copy.component\" \"cd sample && npx react-scripts start\"",
    "start": "serve -s build",
    "build": "babel ./src -d lib --copy-files",
    "test": "react-scripts test",
    "precopy.component": "mkdir -p ./sample/src/components/ && cp -r src/* sample/src/components/",
    "copy.component": "copy-and-watch src/* sample/src/components/ --watch",
    "serve": "serve -s build",
    "tag": "git pull && npm version patch && git push && git push --tags",
    "ship": "npm run build && npm publish"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rand0mC0d3r/material-ui-panel.git"
  },
  "bugs": {
    "url": "https://github.com/rand0mC0d3r/material-ui-panel/issues"
  },
  "homepage": "https://github.com/rand0mC0d3r/material-ui-panel/blob/master/README.md",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
